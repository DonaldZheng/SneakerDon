@model IEnumerable<theSneakerDon.Models.Sneaker>


<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>The Sneaker Don</title>
</head>
<body>
    @foreach (var sneaker in Model)
    {
        <div>
            <h2>@sneaker.Name</h2>
            <h3>@sneaker.Price</h3>
            <h4>@sneaker.Category.CategoryName</h4>
        </div>
    }
</body>
</html>

